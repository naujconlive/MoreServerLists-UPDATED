{
	"schemaVersion": 1,
	"id": "moreserverlists",
	"version": "${version}",
	"name": "MoreServerLists",
	"description": "Add as many server lists as you want with MoreServerLists! This Fabric mod allows you to expand your Minecraft server list and manage multiple server lists effortlessly.",
	"authors": [
		"D151l"
	],
	"contact": {
		"sources": "https://github.com/D151l/MoreServerLists",
		"issues": "https://github.com/D151l/MoreServerLists/issues"
	},
	"license": "CC0-1.0",
	"icon": "assets/moreserverlists/icon.png",
	"environment": "*",
	"entrypoints": {
		"client": [
			"de.d151l.moreserverlists.MoreServerListsModClient"
		]
	},
	"mixins": [
		{
			"config": "moreserverlists.client.mixins.json",
			"environment": "client"
		}
	],
	"depends": {
		"fabricloader": ">=0.16.0",
		"minecraft": ">=1.21.5",
		"java": ">=21",
		"fabric-api": "*"
	}
}
